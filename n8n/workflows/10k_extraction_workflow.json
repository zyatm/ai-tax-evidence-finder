[{
  "name": "SEC 10-K Evidence Extraction",
  "active": false,
  "versionId": "2",
  "nodes": [
    {
      "parameters": {},
      "id": "manual-trigger",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "operation": "upload",
        "title": "Upload 10-K PDF",
        "descriptionType": "text",
        "description": "Upload a 10-K PDF document to extract financial evidence",
        "formFields": {
          "values": [
            {
              "fieldLabel": "10-K PDF File",
              "fieldType": "file",
              "requiredField": true,
              "acceptFileTypes": ".pdf"
            }
          ]
        },
        "options": {}
      },
      "id": "file-upload-form",
      "name": "Upload 10-K PDF",
      "type": "n8n-nodes-base.form",
      "typeVersion": 1,
      "position": [460, 300],
      "webhookId": "upload-10k-form"
    },
    {
      "parameters": {
        "operation": "write",
        "fileName": "=/app/10k-inbox/{{ $json['10-K PDF File'].filename }}",
        "options": {}
      },
      "id": "save-pdf",
      "name": "Save PDF to Disk",
      "type": "n8n-nodes-base.writeBinaryFile",
      "typeVersion": 1,
      "position": [680, 300]
    },
    {
      "parameters": {
        "command": "=cd /app && python3 run.py extract \"/app/10k-inbox/{{ $json['10-K PDF File'].filename }}\" --output /app/output"
      },
      "id": "run-extraction",
      "name": "Run Extraction Script",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [900, 300]
    },
    {
      "parameters": {
        "filePath": "=/app/output/{{ $json['10-K PDF File'].filename.replace('.pdf', '').replace('.PDF', '') }}_evidence_binder.xlsx"
      },
      "id": "read-output",
      "name": "Read Excel Output",
      "type": "n8n-nodes-base.readBinaryFile",
      "typeVersion": 1,
      "position": [1120, 300]
    },
    {
      "parameters": {
        "respondWith": "binary",
        "options": {
          "responseHeaders": {
            "entries": [
              {
                "name": "Content-Disposition",
                "value": "=attachment; filename=\"{{ $json['10-K PDF File'].filename.replace('.pdf', '').replace('.PDF', '') }}_evidence_binder.xlsx\""
              },
              {
                "name": "Content-Type",
                "value": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
              }
            ]
          }
        }
      },
      "id": "respond-download",
      "name": "Download Excel",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1340, 300]
    }
  ],
  "connections": {
